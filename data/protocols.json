    },
    {
        "study": "BRAIN MS",
        "usesContrast": true,
        "layout": {
            "leftCard": {
                "sequences": [
                    { "sequence": "ROUTINE BRAIN +", "highlight": false },
                    { "sequence": "T1: AX, COR", "highlight": false },
