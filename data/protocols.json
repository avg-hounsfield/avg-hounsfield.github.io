    {
        "study": "PITUITARY",
        "usesContrast": true,
        "layout": {
            "leftCard": {
                "sequences": [
                    { "sequence": "T1: SAG", "highlight": false },
                    { "sequence": "T1 3mm: COR, SAG", "highlight": false },
                    { "sequence": "T1 3mm POST: COR, SAG", "highlight": true },
                    { "sequence": "T1 BRAIN POST: AX", "highlight": true },
