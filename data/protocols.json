{
  "category": "Neurological Imaging",
  "protocols": [
    {
      "study": "Trigeminal Neuralgia",
      "scanner": ["3T"],
	  "usesContrast": true,
      "sequences": [
        {
          "type": "MRA",
          "planes": ["Axial"],
          "contrast": "No",
          "note": "Circle of Willis screening"
        },
        {
          "type": "FIESTA",
          "planes": ["Axial"],
          "contrast": "No",
          "note": "High-res nerve root evaluation"
        },
        {
          "type": "T1-weighted post-contrast",
          "planes": ["Axial"],
          "contrast": "Yes",
		  "highlight": true,
          "note": "Thin slices for vascular contact"
        }
      ]
    },
    {
      "study": "Brain Tumor Protocol",
      "scanner": ["1.5T", "3T"],
	  "usesContrast": true,
      "sequences": [
        {
          "type": "T2-weighted",
          "planes": ["Axial", "Coronal", "Sagittal"],
          "contrast": "No",
          "note": "Edema and mass effect evaluation"
        },
        {
          "type": "FLAIR",
          "planes": ["Axial", "Coronal"],
          "contrast": "No",
          "note": "Peritumoral changes and gliosis"
        },
        {
          "type": "DWI",
          "planes": ["Axial"],
          "contrast": "No",
          "note": "Restricted diffusion in high-grade lesions"
        },
        {
          "type": "T1-weighted post-contrast",
          "planes": ["Axial", "Coronal", "Sagittal"],
          "contrast": "Yes",
		  "highlight": true,
          "note": "Enhancing tumor margins"
        }
      ]
    },
