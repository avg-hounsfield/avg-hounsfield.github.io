          "protocol_name": "*POST-OP",
          "match_context": ["post-op", "postop", "post-treatment", "treated", "resection", "surgery", "craniotomy", "surveillance"],
          "priority": 1,
          "note": "Post-op takes precedence when treatment history is mentioned"
