{
  "version": "1.0",
  "description": "Maps ACR procedure names to canonical forms and scanner protocols",
  "terminology_notes": {
    "acr_uses": "head, without IV contrast, with IV contrast",
    "institutions_may_use": "brain, w/o contrast, w/ contrast, non-contrast, +C"
  },
  "normalization_rules": {
    "body_part_synonyms": {
      "head": ["brain", "cranial", "intracranial"],
      "cervical spine": ["c-spine", "neck spine"],
      "thoracic spine": ["t-spine", "dorsal spine"],
      "lumbar spine": ["l-spine", "lower spine"],
      "complete spine": ["total spine", "whole spine", "screening spine"],
      "temporomandibular": ["tmj", "jaw joint"],
      "sacroiliac": ["si joint", "sij"]
    },
    "contrast_synonyms": {
      "without IV contrast": ["w/o contrast", "non-contrast", "NC", "without gad", "pre-contrast", "unenhanced"],
      "with IV contrast": ["w/ contrast", "+C", "with gad", "post-gad", "enhanced", "post-contrast"],
      "without and with IV contrast": ["w/ & w/o contrast", "pre and post contrast", "with and without contrast", "w/wo contrast"]
    },
    "modality_synonyms": {
      "MRI": ["MR", "magnetic resonance"],
      "MRA": ["MR angiography", "magnetic resonance angiography"],
      "MRV": ["MR venography"]
    }
  },
  "procedures": {
    "mri_head_no_contrast": {
      "canonical_display": "MRI Brain Without Contrast",
      "acr_name": "MRI head without IV contrast",
      "body_region": "neuro",
      "body_part": "brain",
      "modality": "MRI",
      "contrast": "none",
      "protocol_routes": [
        {
          "protocol_name": "BRAIN",
          "match_context": ["routine", "trauma", "headache", "default"],
          "priority": 1
        },
        {
          "protocol_name": "SEIZURE",
          "match_context": ["seizure", "epilepsy", "convulsion"],
          "priority": 2
        },
        {
          "protocol_name": "TIA",
          "match_context": ["tia", "stroke", "ischemia", "infarct"],
          "priority": 2
        }
      ]
    },
